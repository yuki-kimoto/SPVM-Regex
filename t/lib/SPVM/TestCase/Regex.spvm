  static method replace : int () {
    # Replace
    {
      my $re = Regex->new("abc");
      my $target = copy "ppzabcz";
      
      $re->replace((mutable string)$target, "ABC");
      my $result = $target;
      
      unless ($target eq "ppzABCz") {
        return 0;
      }
    }
    
    # Replace offset
    {
      my $re = Regex->new("abc");
      my $target = copy "ppzabczabczabcz";
      my $offset = 6;
      
      $re->replace((mutable string)$target, "ABC", \$offset);
      my $result = $target;
      
      unless ($target eq "ppzabczABCzabcz") {
        return 0;
      }
    }
    
    # Replace - capture
    {
      my $re = Regex->new("^(\d+)a(\d+)$");
      my $target = copy "123a456";
      my $offset = 0;
      my $length = -1;
      
      my $replace_info = $re->replace((mutable string)$target, "", \$offset, $length);
      my $result = $target;
      
      unless ($replace_info->match->cap1 eq "123") {
        return 0;
      }
      
      unless ($replace_info->match->cap2 eq "456") {
        return 0;
      }
      
      unless ($target eq "") {
        return 0;
      }
    }
    
    # Replace offset \A
    {
      my $target = copy "zabcabcz";
      my $offset = 1;
      my $length = 6;
      
      Re->s((mutable string)$target, ["\Aabc", "g"], "ABC", \$offset);
      
      unless ($target eq "zABCabcz") {
        diag $target;
        return 0;
      }
    }
    
    return 1;
  }
  
  static method replace_cb : int () {
    # Replace callback with capture
    {
      my $re = Regex->new("a(bc)");
      my $target = copy "ppzabcz";
      my $offset = 0;
      
      my $replace_info = $re->replace((mutable string)$target, method : string ($re : Regex, $match : Regex::Match) {
        return "AB" . $match->cap(1) . "C";
      }, \$offset, -1);
      my $result = $target;
      
      unless ($replace_info->replaced_count == 1) {
        return 0;
      }
      
      unless ($target eq "ppzABbcCz") {
        return 0;
      }
    }
    return 1;
  }
  
  static method match_start_and_end : int () {
    # Match start and end
    {
      my $re = Regex->new("abc");
      my $target = copy "ppzabcz";
      my $match = $re->match($target);
      
      unless ($match) {
        return 0;
      }
      
      unless ($match->match_start == 3) {
        return 0;
      }
      unless ($match->match_length == 3) {
        return 0;
      }
    }

    # Match start UTF-8
    {
      my $re = Regex->new("あい[かきくけこ]+う");
      my $target = copy "んんあいかくこうん";
      my $match = $re->match($target);
      
      unless ($match) {
        return 0;
      }
      
      unless ($match->match_start == 6) {
        return 0;
      }
      unless ($match->match_length == 18) {
        return 0;
      }
    }

    # Match start zero width
    {
      my $re = Regex->new("b*c");
      my $target = copy "zzcz";
      my $match = $re->match($target);
      
      unless ($match) {
        return 0;
      }
      
      unless ($match->match_start == 2) {
        return 0;
      }
      unless ($match->match_length == 1) {
        return 0;
      }
    }


    # Match start zero width only
    {
      my $re = Regex->new("b*");
      my $target = copy "zzcz";
      my $match = $re->match($target);
      
      unless ($match) {
        return 0;
      }
      
      unless ($match->match_start == 0) {
        return 0;
      }
      unless ($match->match_length == 0) {
        return 0;
      }
    }
    
    return 1;
  }
  
  static method match_capture : int ()  {
    # Capture zero more only
    {
      my $re = Regex->new("(a*)");
      my $target = copy "aaa";
      my $match = $re->match($target);
      unless ($match->cap(1) eq "aaa") {
        return 0;
      }
    }
    
    # Capture a char
    {
      my $re = Regex->new("a(b)c");
      my $target = copy "zabcz";
      my $match = $re->match($target);
      
      unless ($match) {
        return 0;
      }
      
      unless ($match->cap(1) eq "b") {
        return 0;
      }
    }

    # Capture a char UTF-8
    {
      my $re = Regex->new("あ(い)う");
      my $target = copy "んあいうん";
      my $match = $re->match($target);
      
      unless ($match) {
        return 0;
      }
      
      unless ($match->cap(1) eq "い") {
        return 0;
      }
    }

    # Capture a char zero width
    {
      my $re = Regex->new("a(b?)c");
      my $target = copy "zacz";
      my $match = $re->match($target);
      
      unless ($match) {
        return 0;
      }
      
      unless ($match->cap(1) eq "") {
        return 0;
      }
    }

    # Capture char class-and quantifier
    {
      my $re = Regex->new("\-(\w+)\-");
      my $target = copy "-abc89-";
      my $match = $re->match($target);
      
      unless ($match) {
        return 0;
      }

      
      unless ($match->cap(1) eq "abc89") {
        return 0;
      }
    }

    # Capture many strings 
    {
      my $re = Regex->new("\-(\w+) (\w+) (\w+) (\w+) (\w+)");
      my $target = copy "-abc1 abc2 abc3 abc4 abc5 abc6-";
      my $match = $re->match($target);
      
      unless ($match) {
        return 0;
      }
      
      unless ($match->cap(1) eq "abc1") {
        return 0;
      }

      unless ($match->cap(2) eq "abc2") {
        return 0;
      }

      unless ($match->cap(3) eq "abc3") {
        return 0;
      }

      unless ($match->cap(4) eq "abc4") {
        return 0;
      }

      unless ($match->cap(5) eq "abc5") {
        return 0;
      }
    }

    # Capture zero more only
    {
      my $re = Regex->new("(a*)");
      my $target = copy "aaa";
      my $match = $re->match($target);
      unless ($match->cap(1) eq "aaa") {
        return 0;
      }
    }
    
    return 1;
  }
  
  static method match_char_class_range : int ()  {
    # Match char class-range
    {
      my $re = Regex->new("[0-9]+");
      my $target = copy "z09az";
      
      my $match = $re->match($target);
      
      unless ($match) {
        return 0;
      }
    }

    # Not Match char class-range
    {
      my $re = Regex->new("[0-9a-c]+");
      my $target = copy "zz";
      
      my $match = $re->match($target);
      
      if ($match) {
        return 0;
      }
    }
    
    # .
    {
      my $re = Regex->new("(.+)");
      my $target = copy "abc\ndef";
      
      my $match = $re->match($target);
      
      unless ($match) {
        return 0;
      }
      
      unless ($match->cap(1) eq "abc") {
        return 0;
      }
    }
    
    # . - single line mode
    {
      my $re = Regex->new("(.+)", "s");
      my $target = copy "abc\ndef";
      
      my $match = $re->match($target);
      
      unless ($match) {
        return 0;
      }
      
      unless ($match->cap(1) eq "abc\ndef") {
        return 0;
      }
    }

    return 1;
  }

