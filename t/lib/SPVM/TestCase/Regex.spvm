  static method match_start : int () {
    # Match ^
    {
      # Match ^ - "abbcz"
      {
        my $re = Regex->new("^ab+c");
        my $target = copy "abbcz";
        
        my $match = $re->match($target);
        
        unless ($match) {
          return 0;
        }
      }
      
      # Not Match ^ - "zabbcz"
      {
        my $re = Regex->new("^ab+c");
        my $target = copy "zabbcz";
        
        my $match = $re->match($target);
        
        if ($match) {
          return 0;
        }
      }
      
      # Match \A - "abbcz"
      {
        my $re = Regex->new("\Aab+c");
        my $target = copy "abbcz";
        
        my $match = $re->match($target);
        
        unless ($match) {
          return 0;
        }
      }
      
      # Match \z - offset + length
      {
        my $re = Regex->new("\Aab+c\z");
        my $target = copy "zabbcz";
        
        my $offset = 1;
        my $length = 4;
        my $match = $re->match($target, \$offset, $length);
        
        unless ($match) {
          return 0;
        }
        
        unless ($offset == 5) {
          diag $offset;
          return 0;
        }
      }
      
    }
    
    return 1;
  }
  
  static method match_end : int () {
    # Match $
    {
      # Match $ - "zabbc"
      {
        my $re = Regex->new("ab+c$");
        my $target = copy "zabbc";
        
        my $match = $re->match($target);
        
        unless ($match) {
          return 0;
        }
      }
      
      # Not Match $ - "zabbcz"
      {
        my $re = Regex->new("ab+c$");
        my $target = copy "zabbcz";
        
        my $match = $re->match($target);
        
        if ($match) {
          return 0;
        }
      }
      
      # Match $ - "zabbc"
      {
