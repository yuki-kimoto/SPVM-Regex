  static method re_split : int () {
    {
      my $string = "foo  bar   baz";
      my $splited_strings = Re->split("\s+", $string);
      unless (Array->equals_string($splited_strings, ["foo", "bar", "baz"])) {
        return 0;
      }
    }
    
    {
      my $string = "fooPbarPbaz";
      my $splited_strings = Re->split(["p", "i"], $string);
      unless (Array->equals_string($splited_strings, ["foo", "bar", "baz"])) {
        return 0;
      }
    }
    
    {
      my $string = "foo,bar,baz";
      my $splited_strings = Re->split(",", $string, 2);
      unless (Array->equals_string($splited_strings, ["foo", "bar,baz"])) {
        return 0;
      }
    }
    
    return 1;
  }
  
