  static method match_char_class : int () {

    # Match char class
    {
      {
        my $re = Regex->new("[bcdあ]+");
        my $target = copy "zabcあz";
        
        my $match = $re->match($target);
        
        unless ($match) {
          return 0;
        }
      }
      
      {
        my $re = Regex->new("^[a-c\dx-z]+$");
        my $target = copy "ac09xz";
        my $match = $re->match($target);
        unless ($match) {
          return 0;
        }
      }
    }

    # Match char class-\d
    {
      my $re = Regex->new("[\da]+");
      my $target = copy "z01az";
      
      my $match = $re->match($target);
      
      unless ($match) {
        return 0;
      }
    }

    # Not Match char class
    {
      my $re = Regex->new("[bcd\d]+");
      my $target = copy "ae";
      
      my $match = $re->match($target);
      
      if ($match) {
        return 0;
      }
    }

    # ^[\d\D]+
    {
      my $re = Regex->new("^[\d\D]+$");
      
      my $target = copy "abc123";
      
      my $match = $re->match($target);
      
      unless ($match) {
        return 0;
      }
    }

    # ^[^\d\D]+
    {
      my $re = Regex->new("^[^\d\D]+$");
      
      {
        my $target = copy "abc";
        
        my $match = $re->match($target);
        if ($match) {
          return 0;
        }
      }
      {
        my $target = copy "123";
        
        my $match = $re->match($target);
        if ($match) {
          return 0;
        }
      }
    }

    # [a-a] | "-" is a character unless "-" is used as a charcter class range
    {
      my $re = Regex->new("[a-a]");
      
      {
        my $target = copy "a";
        my $match = $re->match($target);
        unless ($match) {
          return 0;
        }
      }
    }
    
    # [a-aa-a] | "-" is a character unless "-" is used as a charcter class range
    {
      my $re = Regex->new("[a-aa-a]");
      
      {
        my $target = copy "a";
        my $match = $re->match($target);
        unless ($match) {
          return 0;
        }
      }
    }

    # - | "-" is a character unless "-" is used as a charcter class range
    {
      {
        my $re = Regex->new("-");
        
        {
          my $target = copy "-";
          my $match = $re->match($target);
          unless ($match) {
            return 0;
          }
        }
      }
      {
        my $re = Regex->new("a-z");
        
        {
          my $target = copy "a-z";
          my $match = $re->match($target);
          unless ($match) {
            return 0;
          }
        }
      }
      
    }

    # [-] | "-" is a character unless "-" is used as a charcter class range
    {
      my $re = Regex->new("[-]");
      
      {
        my $target = copy "-";
        my $match = $re->match($target);
        unless ($match) {
          return 0;
        }
      }
    }

    # [a-] | "-" is a character unless "-" is used as a charcter class range
    {
      my $re = Regex->new("[a-]");
      
      {
        my $target = copy "a";
        my $match = $re->match($target);
        unless ($match) {
          return 0;
        }
      }

      {
        my $target = copy "-";
        my $match = $re->match($target);
        unless ($match) {
          return 0;
        }
      }
    }

    # Invalid character class range
    {
      eval { Regex->new("[z-a]"); };
      
      unless ($@) {
        return 0;
      }
    }

    # [ ^a]
    {
      my $re = Regex->new("[ ^a]");
      
      {
        my $target = copy " ";
        my $match = $re->match($target);
        unless ($match) {
          return 0;
        }
      }
      {
        my $target = copy "^";
        my $match = $re->match($target);
        unless ($match) {
          return 0;
        }
      }
      {
        my $target = copy "a";
        my $match = $re->match($target);
        unless ($match) {
          return 0;
        }
      }
    }
    
    
    
    return 1;
  }

