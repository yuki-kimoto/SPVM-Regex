class TestCase::Regex {
  use Regex;
  use Regex::ReplaceInfo;
  use StringBuffer;
  use Re;
  
  static method new : int () {
    
    {
      my $re = Regex->new("abc");
    }
    
    # Exceptions
    {
      # Invalid regex
      {
        eval { Regex->new(")abc"); };
        unless ($@) {
          return 0;
        }
        
        eval { Regex->new("((abc)"); };
        unless ($@) {
          return 0;
        }
      }
    }
    return 1;
  }

  static method replace_g : int () {
    # Replace all
    {
      my $re = Regex->new("abc");
      my $target = copy "ppzabczabcz";
      my $offset = 0;
      my $length = -1;
      my $replace_info = $re->replace_g((mutable string)$target, "ABC", \$offset, $length);
      
      unless ($replace_info->replaced_count == 2) {
        return 0;
      }
      
      unless ($target eq "ppzABCzABCz") {
        return 0;
      }
    }

    # Replace all offset
    {
      my $re = Regex->new("abc");
      my $target = copy "ppzabczabczabcz";
      my $offset = 6;
      my $length = -1;
      
      my $replace_info = $re->replace_g((mutable string)$target, "ABC", \$offset, $length);
      unless ($replace_info->replaced_count == 2) {
        return 0;
      }
      
      unless ($target eq "ppzabczABCzABCz") {
        return 0;
      }
    }

    # Replace all with number
    {
      my $re = Regex->new("a\d+b");
      my $target = copy "za123bz";
      my $offset = 0;
      my $length = -1;
      
      my $replace_info = $re->replace_g((mutable string)$target, "ABC", \$offset, $length);
      
      unless ($replace_info->replaced_count == 1) {
        return 0;
      }
      
      unless ($target eq "zABCz") {
        return 0;
      }
    }

    # Replace all - Not replace_g
    {
      my $re = Regex->new("a\d+b");
      my $target = copy "pppp";
      my $offset = 0;
      my $length = -1;
      
      my $replace_info = $re->replace_g((mutable string)$target, "ABC", \$offset, $length);
      
      if ($replace_info) {
        return 0;
      }
      
      unless ($target eq "pppp") {
        return 0;
      }
    }
    
    return 1;
  }
  
